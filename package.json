{
  "name": "pigs-ui",
  "version": "0.0.3",
  "description": "UI Library for Nerv",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.config.dev.js --open --content src --log-level warn",
    "dist": "node build/build.js dist",
    "dist:report": "node build/build.js dist report",
    "build": "node build/build.js build",
    "build:report": "node build/build.js build report"
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "prettier --config .prettierrc --write",
      "eslint",
      "git add"
    ],
    "src/**/*.(ts|tsx)": [
      "prettier --config .prettierrc --write",
      "tslint",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --config .prettierrc --write",
      "stylelint --syntax=scss",
      "git add"
    ],
    "src/**/*.css": [
      "prettier --config .prettierrc --write",
      "stylelint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:sun.yuhang/pigs-ui.git"
  },
  "keywords": [
    "Pigs-UI",
    "Nerv"
  ],
  "author": "sunyuhang",
  "main": "lib/index.js",
  "homepage": "https://sunyuhang404.github.io/pigs-ui/",
